{
    "name": "alyakin/dictionary-cache-service",
    "description": "Dictionary caching service for Redis-compatible stores (Redis, KeyDB, Valkey, etc.).",
    "keywords": ["redis", "cache", "dictionary", "caching", "keydb", "valkey", "dragonfly", "ardb"],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Alexander Lyakin",
            "email": "2177866@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0 || ^10.0",
        "mockery/mockery": "^1.5",
        "laravel/pint": "^1.25",
        "phpstan/phpstan": "^2.1"
    },
    "suggest": {
        "ext-redis": "Use the PhpRedisClient adapter when running outside Laravel",
        "illuminate/redis": "Required for the IlluminateRedisClient adapter or Laravel service provider",
        "illuminate/support": "Required only when using the Laravel service provider binding"
    },
    "autoload": {
        "psr-4": {
            "Alyakin\\DictionaryCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alyakin\\DictionaryCache\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "lint": "pint",
        "stan": "phpstan analyse",
        "test": "phpunit",
        "check": [
            "@lint",
            "@stan",
            "@test"
        ]
    }
}
